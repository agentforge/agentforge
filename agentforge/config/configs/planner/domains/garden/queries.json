{
  "queries": [
    {
      "id": "plant",
      "query": "What kind of plants do you want to grow?",
      "relation": "[ENT] [R1] wants to grow [ENT] [R2]",
      "type": "string",
      "effect": [
        {
          "val":"(growing {val})",
          "type": "goal"
        },
        {
          "val": "{val} - plant",
          "type": "object"
        },
        {
          "val": "{val}-seed - seed",
          "type": "object"
        },
        {
          "val": "{val}-seedling - seedling",
          "type": "object"
        },
        {
          "val": "(has-seed-type {val}-seed {val})",
          "type": "init"
        },
        {
          "val": "(has-seedling-type {val}-seedling {val})",
          "type": "init"
        },
        {
          "val": "fertilizer - fertilizer",
          "type": "object"
        },
        {
          "val": "shovel - tool",
          "type": "object"
        }
      ]
    },
    {
      "id": "plot",
      "query": "Do you have a plot available to you?",
      "relation": "[ENT] [R1] has a [ENT] [R2]",
      "subject": "plot",
      "type": "boolean",
      "effect": [
        {
          "val": "plotX - plot",
          "type": "object"
        },
        {
          "val": "(dry plotX)",
          "type": "init"
        },
        {
          "val": "(unplanted plotX)",
          "type": "init"
        }
      ]
    },
    {
      "id": "container",
      "query": "Do you have containers for growing plants?",
      "relation": "[REL] [ENT1] has a [REL] [ENT2]",
      "subject": "container",
      "type": "boolean",
      "effect": [
        {
          "val": "containerX - container",
          "type": "object"
        },
        {
          "val": "(container-available containerX)",
          "type": "init"
        }
      ]
    }
  ]
}